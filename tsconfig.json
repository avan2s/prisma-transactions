{
    "compilerOptions": {
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "target": "ES2020",
        "module": "CommonJS",
        "declaration": true,
        "outDir": "./dist",
	"declarationMap": true,
	"baseUrl": "./",
	"removeComments": true,
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "sourceMap": true
    },
    "include": ["src","src/*", "prisma"]
}
